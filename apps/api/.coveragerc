[run]
source = app
omit = 
    */tests/*
    */__pycache__/*
    */migrations/*
    */conftest.py
    */__init__.py

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
title = TesiGo API Coverage Report

